<table>
 <h1>検索結果:<%= @rooms.count %>件</h1> 
   <thead>
    <tr>
     <th>ルームID</th>
     <th>ルーム名</th>
     <th>ルーム紹介</th>
     <th>料金</th>
     <th>登録日</th>
     <th>住所</th>
     <th>投稿者ID</th>
     <th>予約</th>
     <th>編集</th>
     <th>削除</th>
    </tr>
   </thead>
   <tbody>
    <% @rooms.each do |room| %>
    <tr>
      <td><%= room.id %></td>
      <td><%= room.room_name %></td>
      <td><%= room.comment %></td>
      <td><%= room.price %>円</td>
      <td><%= room.created_at.strftime("%Y/%m/%d") %></td>
      <td><%= room.address %></td>
      <td><%= room.user_id %></td>
      <td><%= link_to"予約する",room %></td>
      <td><%= link_to"編集する",edit_room_path(room.id) %></td>
      <td><%= link_to"削除する", room_path(room.id), method: :delete, data: { confirm: "本当に削除しますか？" } %></td>
   </tbody>  
    </tr>
  <% end %>
</table>